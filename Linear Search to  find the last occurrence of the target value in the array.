#include <iostream>
using namespace std;

int main()
{
     int n;
     cout<<"enter the size: ";
     cin>> n;
     
     int arr[n];
     cout<<"enter the array: ";
     for(int i=0; i<n; i++){
         cin>> arr[i];
     }
     int target ;
     cout<<"enter target value: ";
     cin>> target;
     int flag =-1; 
     for(int i=0; i<n; i++){
         if(arr[i]==target){
             
             flag = i;
             
              
         }
     }
     if(flag != -1){
         cout<<"Last Occurrence of the Key at the index:" <<flag;
     }
     else{
        cout<<"Not found";
     }
     
      

    return 0;
}
