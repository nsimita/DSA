#include <iostream>
using namespace std;

int main (){
     int n;
     cout<<"enter size: ";
     cin>>n;
     int a[n];
     for(int i=0; i<n; i++){
           cin>> a[i];  //taking input array
     }
     int f[101] = {0}; // frequncy count array
      
     for(int j=0; j<n; j++){
         f[a[j]]++;    //frequency count
     }

     for(int i=0; i<100; i++)
       if(f[i]>0){
         cout<< i << " occures " << f[i] <<" ";
       }

     }

    

