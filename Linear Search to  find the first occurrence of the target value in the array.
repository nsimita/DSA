#include <iostream>
using namespace std;

int main()
{
     int n;
     cout<<"enter the size: ";
     cin>> n;
     
     int arr[n];
     cout<<"enter the array: ";
     for(int i=0; i<n; i++){
         cin>> arr[i];
     }
     int target ;
     cout<<"enter target value: ";
     cin>> target;
     int flag =0; 
     for(int i=0; i<n; i++){
         if(arr[i]==target){
             
             flag =1;
             cout<< "First Occurrence of the Key at the index: " <<i;
             break;
         }
     }
     if(flag !=1){
         cout<<"not found";
     }
     
      

    return 0;
}
