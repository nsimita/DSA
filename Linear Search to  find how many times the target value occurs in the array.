#include <iostream>
using namespace std;

int main()
{
     int n;
     cout<<"enter the size: ";
     cin>> n;
     
     int arr[n];
     cout<<"enter the array: ";
     for(int i=0; i<n; i++){
         cin>> arr[i];
     }
     int target ;
     cout<<"enter target value: ";
     cin>> target;
     int count = 0; 
     for(int i=0; i<n; i++){
         if(arr[i]==target){
             count++;
             }
     }
     if(count != 0){
         cout<<" The target value occurs: " <<count;
     }
     else{
        cout<<"The target value occurs: "<<count;
     }
     
      

    return 0;
}
