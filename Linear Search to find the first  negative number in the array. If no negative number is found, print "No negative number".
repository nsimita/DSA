#include <iostream>
using namespace std;

int main()
{
     int n;
     cout<<"enter the size: ";
     cin>> n;
     
     int arr[n];
     cout<<"enter the array: ";
     for(int i=0; i<n; i++){
         cin>> arr[i];
     }
     
     int flag =0; 
     for(int i=0; i<n; i++){
         if(arr[i] < 0){
              cout<<arr[i];
              flag = 1;
             break;
             }
              
     }
     if(flag != 1){
        cout<<"no negative found";
     }
     
 
    return 0;
}
